{
  "name": "Flode",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "npx react-native run-ios",
    "start": "npx react-native start",
    "test": "jest",
    "test:update": "jest --verbose --coverage --updateSnapshot",
    "test:watch": "jest --verbose --watch",
    "coverage": "jest --verbose --coverage && open ./coverage/lcov-report/index.html",
    "lint": "eslint .",
    "e2e:ios": "detox build --configuration ios && detox test --configuration ios"
  },
  "dependencies": {
    "@react-native-community/blur": "^3.6.0",
    "@react-native-community/masked-view": "^0.1.11",
    "@react-native-firebase/app": "^12.1.0",
    "@react-native-firebase/firestore": "^12.1.0",
    "@react-native-firebase/storage": "^12.2.0",
    "@react-navigation/bottom-tabs": "^5.11.11",
    "@react-navigation/drawer": "^6.1.8",
    "@react-navigation/native": "^5.9.4",
    "@react-navigation/stack": "^5.14.5",
    "@testing-library/jest-native": "^4.0.1",
    "@testing-library/react-native": "^7.2.0",
    "@types/react-redux": "^7.1.18",
    "formik": "^2.2.9",
    "lodash": "^4.17.21",
    "react": "17.0.1",
    "react-native": "0.64.2",
    "react-native-fast-image": "^8.3.7",
    "react-native-gesture-handler": "^1.10.3",
    "react-native-image-picker": "^4.0.4",
    "react-native-linear-gradient": "^2.5.6",
    "react-native-reanimated": "^2.0.0",
    "react-native-safe-area-context": "^3.2.0",
    "react-native-screens": "^3.4.0",
    "react-native-svg": "^12.1.1",
    "react-native-vector-icons": "^8.1.0",
    "react-redux": "^7.2.4",
    "redux": "^4.1.0",
    "redux-devtools-extension": "^2.13.9",
    "redux-saga": "^1.1.3",
    "redux-thunk": "^2.3.0",
    "ts-jest": "27.0.0",
    "yup": "^0.32.9"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "@trivago/prettier-plugin-sort-imports": "^3.1.1",
    "@types/jest": "^26.0.23",
    "@types/react": "^17.0.13",
    "@types/react-native": "^0.64.10",
    "@types/react-native-vector-icons": "^6.4.8",
    "@types/react-test-renderer": "^17.0.1",
    "babel-jest": "^26.6.3",
    "babel-plugin-module-resolver": "^3.2.0",
    "detox": "^18.20.0",
    "eslint": "7.14.0",
    "eslint-import-resolver-babel-module": "^5.1.0",
    "eslint-plugin-import": "^2.18.2",
    "jest": "^26.6.3",
    "jest-circus": "^27.0.6",
    "jest-transform-stub": "^2.0.0",
    "metro-react-native-babel-preset": "^0.64.0",
    "react-navigation": "^4.3.7",
    "react-navigation-stack": "^2.3.11",
    "react-test-renderer": "17.0.1",
    "typescript": "^4.3.5"
  },
  "jest": {
    "preset": "react-native",
    "setupFiles": [
      "./jest/setup.js",
      "./node_modules/react-native-gesture-handler/jestSetup.js"
    ],
    "cacheDirectory": "./cache",
    "coveragePathIgnorePatterns": [
      "./app/utils/vendor"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 80
      }
    },
    "transformIgnorePatterns": [
      "/node_modules/(?!react-native|react-clone-referenced-element|react-navigation)"
    ]
  }
}
